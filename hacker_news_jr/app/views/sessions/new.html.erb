<h1>Login</h1>

<br>
<h3>Login</h3>
<%= form_for @user, url: '/login' do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit 'login' %>
<% end %>

<br>
<h3>Create Account</h3>
<%= render template: "users/new" %>
